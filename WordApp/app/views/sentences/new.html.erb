<div class="container-fluid">
  <div class="row">
      
	  <div id ="punctuation" class="col-md-1">
		<% @punctuation.each_with_index do |x, i|%>
			<p id="punc<%=i%>" class='ui-widget-header'><%= x.content%></p>
	 	<%end%>

	</div>

	 <div class="col-md-9" style="border:1px solid black">

 		<div id="sentence_parts" class="row">
		</div>

		 <div id="words_holder" class="row">
		 	<div id="words" >
		 	</div>
		 </div>

	 </div>



	<div class="col-md-1" style="border:1px solid black">


	  <div id ="suffixes">
		<% @suffixes.each do |x|%>
			<p id="suffix"><%= x.content%></p>
		<%end%>
    </div>


    </div>  
  </div>
</div>



<%= render 'form'%>

